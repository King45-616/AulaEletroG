#include<stdio.h>

int main() {
    int copias;
    float total;

    printf("Digite o numero de copias: ");
    scanf("%d", &copias);

    if (copias <= 100) {
        total = copias * 0.25;
    } else {
        total = copias * 0.20;
    }

    printf("Total a pagar: R$ %.2f\n", total);

    return 0;
} 
